<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:background="@android:color/black"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:layout_gravity="center">
	<RelativeLayout
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:id="@+id/relativeLayout1">
		<ImageButton
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:src="@drawable/ic_btn_back"
			android:id="@+id/imageButton1"
			android:layout_alignParentLeft="true"></ImageButton>
		<TextView
			android:layout_height="fill_parent"
			android:layout_width="wrap_content"
			android:id="@+id/conferenceTitle"
			android:text="Schedule"
			android:layout_alignWithParentIfMissing="true"
			android:gravity="center_vertical"
			android:layout_toRightOf="@+id/imageButton1"
			android:layout_alignTop="@+id/imageButton1"
			android:layout_alignBottom="@+id/imageButton1"
			android:textSize="@dimen/textsize_title"></TextView>
		<ImageButton
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/ic_btn_next"
			android:id="@+id/imageButton5"
			android:layout_toLeftOf="@+id/imageButton4"
			android:layout_alignTop="@+id/imageButton4"
			android:layout_alignBottom="@+id/imageButton4" android:visibility="gone"></ImageButton>
		<ImageButton
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/ic_btn_next"
			android:id="@+id/imageButton4"
			android:layout_alignParentRight="true" android:visibility="gone"></ImageButton>
	</RelativeLayout>
	<View
		android:id="@+id/firstDivider"
		android:layout_height="2dp"
		android:layout_width="fill_parent"
		android:background="@color/linebreak" />
	<FrameLayout
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:id="@+id/frameLayout1">
		<TextView
			android:layout_gravity="center"
			android:layout_width="wrap_content"
			android:text="23 mei 1999"
			android:layout_height="wrap_content" android:id="@+id/conferenceDate"></TextView>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView1"
			android:layout_gravity="right"
			android:text="Right" android:paddingRight="3dp"></TextView>
		<TextView
			android:id="@+id/textView2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Left" android:layout_gravity="left" android:paddingLeft="3dp"></TextView>
	</FrameLayout>
	<View
		android:id="@+id/secondDivider"
		android:layout_height="2dp"
		android:layout_width="fill_parent"
		android:background="@color/linebreak" />
	<FrameLayout android:id="@+id/FrameLayout01" android:layout_width="fill_parent" android:layout_height="wrap_content">
		<LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/linearLayout1" android:layout_gravity="right">
			<ImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_btn_search" android:id="@+id/sessionEditButton"></ImageButton>
			<ImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_btn_search" android:id="@+id/sessionMarkButton"></ImageButton>
		</LinearLayout>
		<LinearLayout android:layout_width="wrap_content" android:orientation="vertical" android:layout_height="wrap_content" android:id="@+id/linearLayout2" android:layout_gravity="center">
			<TextView android:layout_gravity="center" android:layout_width="wrap_content" android:text="17.00u - 18.00u" android:layout_height="wrap_content" android:textSize="18sp" android:id="@+id/sessionTime"></TextView>
			<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center" android:text="in 10 minutes" android:id="@+id/sessionTimeRemark"></TextView>
		</LinearLayout>
	</FrameLayout>
	<LinearLayout android:layout_width="fill_parent" android:id="@+id/sessionContentLayout" android:layout_height="wrap_content"></LinearLayout>
	<View android:layout_height="2dp" android:layout_width="fill_parent" android:background="@color/linebreak" android:id="@+id/thirdDivider" android:layout_gravity="center"></View>
	<LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout3" android:layout_width="fill_parent" android:layout_gravity="center" android:gravity="center">
		<ImageView android:layout_height="wrap_content" android:id="@+id/ImageView03" android:src="@drawable/ic_btn_back" android:layout_width="wrap_content"></ImageView>
		<ImageView android:layout_height="wrap_content" android:id="@+id/ImageView02" android:src="@drawable/ic_btn_back" android:layout_width="wrap_content"></ImageView>
		<ImageView android:layout_height="wrap_content" android:id="@+id/ImageView01" android:layout_width="wrap_content" android:src="@drawable/ic_btn_next"></ImageView>
		<ImageView android:layout_height="wrap_content" android:id="@+id/imageView1" android:layout_width="wrap_content" android:src="@drawable/ic_btn_next"></ImageView>
	</LinearLayout>
</LinearLayout>