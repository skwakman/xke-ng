<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- Header part of the session -->
	<LinearLayout
		android:orientation="vertical"
		android:background="@android:color/black"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentTop="true"
		android:id="@+id/sc_header">
		<RelativeLayout
			android:layout_height="wrap_content"
			android:layout_width="fill_parent"
			android:id="@+id/relativeLayout1">
			<ImageView
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:src="@drawable/x_conference"
				android:id="@+id/conferenceButton"
				android:layout_alignParentLeft="true"></ImageView>
			<TextView
				android:layout_height="fill_parent"
				android:layout_width="wrap_content"
				android:paddingLeft="7sp"
				android:layout_alignWithParentIfMissing="true"
				android:gravity="center_vertical"
				android:layout_toRightOf="@+id/conferenceButton"
				android:layout_alignTop="@+id/conferenceButton"
				android:layout_alignBottom="@+id/conferenceButton"
				android:textSize="@dimen/textsize_title"
				android:text="Empty"
				android:id="@+id/addModifyTitle"></TextView>
			<ImageButton
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/x_conference"
				android:id="@+id/imageButton5"
				android:layout_toLeftOf="@+id/imageButton4"
				android:layout_alignTop="@+id/imageButton4"
				android:layout_alignBottom="@+id/imageButton4"
				android:visibility="gone"></ImageButton>
			<ImageButton
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/x_conference"
				android:id="@+id/imageButton4"
				android:layout_alignParentRight="true"
				android:visibility="gone"></ImageButton>
		</RelativeLayout>
		<View
			android:id="@+id/firstDivider"
			android:layout_height="2dp"
			android:layout_width="fill_parent"
			android:background="@color/linebreak" />
	</LinearLayout>
	<!-- Footer part of the session -->
	<LinearLayout
		android:orientation="vertical"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:id="@+id/sc_navigate">
		<View
			android:layout_height="2dp"
			android:layout_width="fill_parent"
			android:background="@color/linebreak"
			android:id="@+id/thirdDivider" />
		<LinearLayout
			android:layout_height="wrap_content"
			android:id="@+id/linearLayout3"
			android:layout_width="fill_parent"
			android:gravity="center">
			<Button
				android:text="Save"
				android:layout_width="wrap_content"
				android:id="@+id/actionSave"
				android:layout_height="wrap_content"></Button>
			<Button
				android:text="Reschedule"
				android:layout_width="wrap_content"
				android:id="@+id/actionReschedule"
				android:layout_height="wrap_content"></Button>
			<Button
				android:text="Delete"
				android:layout_width="wrap_content"
				android:id="@+id/actionDelete"
				android:layout_height="wrap_content"></Button>
		</LinearLayout>
	</LinearLayout>
	<!-- Content part of the session -->
	<ScrollView
		android:id="@+id/sc_content"
		android:layout_below="@id/sc_header"
		android:layout_above="@id/sc_navigate"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<LinearLayout
			android:orientation="vertical"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			<TextView
				android:background="@drawable/rateslider"
				android:textSize="@dimen/ts_group"
				android:id="@+id/TextView11"
				android:layout_width="fill_parent"
				android:paddingRight="5sp"
				android:paddingLeft="5sp"
				android:textColor="@color/textcolor_header"
				android:paddingBottom="2sp"
				android:layout_height="wrap_content"
				android:paddingTop="2sp"
				android:text="Planning"></TextView>
			<TableLayout
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:id="@+id/TableLayout01"
				android:padding="6sp">
				<TableRow
					android:layout_width="wrap_content"
					android:id="@+id/TableRow06"
					android:layout_height="wrap_content">
					<TextView
						android:id="@+id/TextView07"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Title"></TextView>
					<TextView
						android:text="@string/empty_value"
						android:padding="1sp"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5sp"
						android:layout_width="wrap_content"
						android:id="@+id/conferenceName"></TextView>
				</TableRow>
				<TableRow
					android:layout_width="wrap_content"
					android:id="@+id/TableRow04"
					android:layout_height="wrap_content">
					<TextView
						android:id="@+id/TextView14"
						android:text="Date"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"></TextView>
					<TextView
						android:text="@string/empty_value"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5sp"
						android:layout_width="wrap_content"
						android:id="@+id/conferenceDate"
						android:background="@drawable/touchtext_disable"></TextView>
				</TableRow>
				<TableRow
					android:layout_width="wrap_content"
					android:id="@+id/TableRow05"
					android:layout_height="wrap_content">
					<TextView
						android:id="@+id/TextView12"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Start time"></TextView>
					<TextView
						android:text="@string/empty_value"
						android:padding="1sp"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5sp"
						android:layout_width="wrap_content"
						android:id="@+id/conferenceStart"></TextView>
				</TableRow>
				<TableRow
					android:layout_width="wrap_content"
					android:id="@+id/TableRow07"
					android:layout_height="wrap_content">
					<TextView
						android:id="@+id/TextView10"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="End time"></TextView>
					<TextView
						android:text="@string/empty_value"
						android:padding="1sp"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5sp"
						android:layout_width="wrap_content"
						android:id="@+id/conferenceEnd"></TextView>
				</TableRow>
			</TableLayout>
			<TextView
				android:background="@drawable/rateslider"
				android:textSize="@dimen/ts_group"
				android:id="@+id/TextView16"
				android:layout_width="fill_parent"
				android:paddingRight="5sp"
				android:paddingLeft="5sp"
				android:textColor="@color/textcolor_header"
				android:paddingBottom="2sp"
				android:layout_height="wrap_content"
				android:paddingTop="2sp"
				android:text="Details"></TextView>
			<TableLayout
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:id="@+id/TableLayout02"
				android:padding="6sp">
				<TableRow
					android:layout_width="wrap_content"
					android:id="@+id/TableRow01"
					android:layout_height="wrap_content">
					<TextView
						android:id="@+id/TextView17"
						android:text="Description"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"></TextView>
					<TextView
						android:text="@string/empty_value"
						android:padding="1sp"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5sp"
						android:layout_width="wrap_content"
						android:id="@+id/conferenceDescription"></TextView>
				</TableRow>
				<TableRow
					android:layout_width="wrap_content"
					android:id="@+id/TableRow09"
					android:layout_height="wrap_content">
					<TextView
						android:id="@+id/TextView15"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Organiser"></TextView>
					<TextView
						android:text="@string/empty_value"
						android:padding="1sp"
						android:textSize="@dimen/ts_attribute"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5sp"
						android:layout_width="wrap_content"
						android:id="@+id/conferenceOrganiser"></TextView>
				</TableRow>
				<TableRow
					android:id="@+id/TableRow12"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content">
					<TextView
						android:id="@+id/TextView24"
						android:layout_width="wrap_content"
						android:text="Locations"
						android:layout_height="wrap_content"
						android:textSize="@dimen/ts_attribute"></TextView>
					<TextView
						android:padding="1sp"
						android:id="@+id/conferenceLocations"
						android:layout_marginLeft="5sp"
						android:layout_width="wrap_content"
						android:text="@string/empty_value"
						android:layout_height="wrap_content"
						android:textSize="@dimen/ts_attribute"></TextView>
				</TableRow>
			</TableLayout>
		</LinearLayout>
	</ScrollView>
</RelativeLayout>