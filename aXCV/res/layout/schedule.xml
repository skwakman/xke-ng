<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:background="@android:color/black"
	android:id="@+id/scheduleSwipeBase"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<RelativeLayout
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:id="@+id/relativeLayout1">
		<ImageView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:src="@drawable/x_conference"
			android:id="@+id/conferenceButton"
			android:layout_alignParentLeft="true"></ImageView>
		<TextView
			android:paddingLeft="7sp"
			android:layout_height="fill_parent"
			android:layout_width="wrap_content"
			android:id="@+id/conferenceTitle"
			android:text="Schedule"
			android:layout_alignWithParentIfMissing="true"
			android:gravity="center_vertical"
			android:layout_toRightOf="@+id/conferenceButton"
			android:layout_alignTop="@+id/conferenceButton"
			android:layout_alignBottom="@+id/conferenceButton"
			android:textSize="@dimen/textsize_title"></TextView>
		<ImageButton
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/x_conference"
			android:id="@+id/imageButton5"
			android:layout_toLeftOf="@+id/imageButton4"
			android:layout_alignTop="@+id/imageButton4"
			android:visibility="gone"
			android:layout_alignBottom="@+id/imageButton4"></ImageButton>
		<ImageButton
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/x_conference"
			android:id="@+id/imageButton4"
			android:visibility="gone"
			android:layout_alignParentRight="true"></ImageButton>
	</RelativeLayout>
	<View
		android:id="@+id/firstDivider"
		android:layout_height="2dp"
		android:layout_width="fill_parent"
		android:background="@color/linebreak" />
		<FrameLayout
			android:layout_height="wrap_content"
			android:layout_width="fill_parent"
			android:id="@+id/frameLayout1">
			<TextView
				android:layout_gravity="center"
				android:layout_width="wrap_content"
				android:text="23 mei 1999"
				android:layout_height="wrap_content"
				android:id="@+id/conferenceDate"></TextView>
			<LinearLayout
				android:id="@+id/nextLocationLayout"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="right">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Right"
					android:paddingRight="3dp" android:id="@+id/rightText"></TextView>
				<ImageView
					android:id="@+id/ImageView04"
					android:layout_width="wrap_content"
					android:src="@drawable/arrow_right_float"
					android:layout_height="fill_parent"></ImageView>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/prevLocationLayout"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="left">
				<ImageView
					android:id="@+id/ImageView04"
					android:layout_width="wrap_content"
					android:src="@drawable/arrow_left_float"
					android:layout_height="fill_parent"></ImageView>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Left"
					android:layout_gravity="left"
					android:paddingLeft="3dp" android:id="@+id/leftText"></TextView>
			</LinearLayout>
		</FrameLayout>
	<View
		android:id="@+id/secondDivider"
		android:layout_height="2dp"
		android:layout_width="fill_parent"
		android:background="@color/linebreak" />
	<ListView
		android:id="@+id/sessionList"
		android:layout_height="fill_parent"
		android:layout_width="fill_parent"></ListView>
</LinearLayout>