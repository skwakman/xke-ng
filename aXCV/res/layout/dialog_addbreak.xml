<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:id="@+id/linearLayout1"
	android:layout_height="fill_parent"
	android:layout_width="fill_parent">
    <TextView android:id="@+id/seDescription" android:layout_gravity="center_horizontal" android:layout_marginTop="10sp" android:textSize="18sp" android:layout_marginBottom="10sp" android:layout_width="wrap_content" android:gravity="center" android:paddingLeft="5sp" android:layout_height="wrap_content" android:paddingRight="5sp" android:text="@string/breakLabel"></TextView>
    <EditText android:id="@+id/seValue" android:layout_gravity="center_horizontal" android:layout_marginLeft="5sp" android:inputType="text|textAutoCorrect|textAutoComplete|textShortMessage" android:layout_width="fill_parent" android:maxLength="25" android:layout_height="42dp" android:cursorVisible="true" android:gravity="top|left" android:layout_marginRight="5sp">
        <requestFocus></requestFocus>
    </EditText>
    <TableLayout android:layout_height="wrap_content" android:padding="6sp" android:id="@+id/TableLayout01" android:layout_width="fill_parent">
        <TableRow android:id="@+id/TableRow05" android:layout_width="wrap_content" android:layout_height="wrap_content" android:padding="4sp">
            <TextView android:id="@+id/TextView12" android:layout_height="wrap_content" android:textSize="@dimen/ts_attribute" android:layout_width="wrap_content" android:text="Start" android:layout_gravity="center_vertical"></TextView>
            <Spinner android:layout_weight="1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/bStartTime"></Spinner>
        </TableRow>
        <TableRow android:id="@+id/TableRow07" android:layout_width="wrap_content" android:layout_height="wrap_content" android:padding="4sp">
            <TextView android:id="@+id/TextView10" android:layout_height="wrap_content" android:textSize="@dimen/ts_attribute" android:layout_width="wrap_content" android:text="Duration" android:layout_gravity="center_vertical"></TextView>
            <Spinner android:layout_weight="1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/bDuration"></Spinner>
        </TableRow>
        <TableRow android:id="@+id/TableRow12" android:layout_width="wrap_content" android:layout_height="wrap_content" android:padding="4sp">
            <TextView android:id="@+id/TextView24" android:layout_height="wrap_content" android:textSize="@dimen/ts_attribute" android:layout_width="wrap_content" android:text="Location" android:layout_gravity="center_vertical"></TextView>
            <Spinner android:layout_weight="1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/bLocation"></Spinner>
        </TableRow>
    </TableLayout>
	<LinearLayout
		android:id="@+id/linearLayout2"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal"
		android:gravity="center_horizontal">
		<Button
			android:text="Submit"
			android:layout_width="wrap_content"
			android:layout_gravity="center_horizontal"
			android:id="@+id/seCommit"
			android:layout_height="wrap_content"></Button>
		<Button
			android:layout_width="wrap_content"
			android:layout_gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:id="@+id/seCancel"
			android:text="Cancel"></Button>
	</LinearLayout>
</LinearLayout>