<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:stretchColumns="1"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical">
	<View
		android:layout_width="fill_parent"
		android:id="@+id/tabDivider"
		android:background="@color/linebreak"
		android:layout_height="1dp"></View>
	<TextView android:layout_height="wrap_content" android:paddingLeft="3dp" android:layout_width="wrap_content" android:paddingRight="3dp" android:id="@+id/itemtext" android:textSize="@dimen/ts_sessiontitle" android:text="Message"></TextView>
</LinearLayout>