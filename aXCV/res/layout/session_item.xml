<?xml version="1.0" encoding="utf-8"?>
<TableLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:stretchColumns="1"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<LinearLayout
		android:id="@+id/linearLayout1"
		android:orientation="vertical"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<LinearLayout
			android:layout_height="fill_parent"
			android:id="@+id/linearLayout2"
			android:layout_width="fill_parent"
			android:orientation="horizontal">
			<LinearLayout
				android:layout_height="fill_parent"
				android:id="@+id/linearLayout3"
				android:orientation="vertical"
				android:layout_width="fill_parent"
				android:layout_weight="2">
				<TextView
					android:textSize="@dimen/ts_sessiontitle"
					android:layout_width="wrap_content"
					android:text="Title"
					android:layout_height="wrap_content"
					android:id="@+id/ses_title"
					android:paddingLeft="3dp"
					android:paddingRight="3dp"></TextView>
				<TextView
					android:textSize="@dimen/ts_sessiondetail"
					android:layout_width="wrap_content"
					android:text="Author"
					android:layout_height="wrap_content"
					android:id="@+id/ses_author"
					android:paddingLeft="3dp"
					android:paddingRight="3dp"></TextView>
				<TextView
					android:textSize="@dimen/ts_sessiondetail"
					android:layout_width="wrap_content"
					android:text="Jun 7th, 2011"
					android:layout_height="wrap_content"
					android:id="@+id/ses_labels"
					android:paddingLeft="3dp"
					android:paddingRight="3dp"></TextView>
			</LinearLayout>
			<TableLayout
				android:id="@+id/tableLayout1"
				android:layout_height="fill_parent"
				android:layout_width="wrap_content">
				<TableRow
					android:id="@+id/tableRow1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">
					<ImageView
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:src="@drawable/ic_menu_edit"
						android:id="@+id/editButton"></ImageView>
					<ImageView
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:id="@+id/markButton"
						android:src="@android:drawable/btn_star_big_off"></ImageView>
				</TableRow>
			</TableLayout>
		</LinearLayout>
		<TextView
			android:gravity="right"
			android:textSize="@dimen/ts_sessionschedule"
			android:textColor="@color/tc_sessionschedule"
			android:text="no open slots"
			android:layout_height="wrap_content"
			android:id="@+id/ses_locdate"
			android:layout_width="fill_parent"
			android:paddingLeft="3dp"
			android:paddingRight="3dp"></TextView>
	</LinearLayout>
	<View
		android:id="@+id/tabDivider"
		android:layout_height="2dp"
		android:layout_width="fill_parent"
		android:background="@color/linebreak" />
</TableLayout>