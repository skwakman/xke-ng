<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<corners
				android:radius="5dip" />
			<solid
				android:color="#000000" />
			<stroke
				android:width="1dip"
				android:color="#4fa5d5" />
			<padding
				android:left="5sp"
				android:top="1sp"
				android:right="5sp"
				android:bottom="1sp" />
		</shape>
	</item>
</layer-list>